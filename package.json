{
  "name": "writtenlyhub-blogs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "generate:sitemap": "node ./scripts/generate-sitemap.mjs",
    "prebuild": "npm run generate:sitemap",
    "build": "vite build",
    "prebuild:prod-blog": "set SITE_URL=https://www.writtenlyhub.com/blog&& set WP_API_BASE_URL=https://www.writtenlyhub.com/wp-json/wp/v2&& npm run generate:sitemap",
